                echo 'stage-1'
                }
            stages{
                stage('Innerstage'){
                    steps{
                        echo 'Innerstage-1'
                }
            }
            }
        }
        stage('stage-2'){
            steps{
                echo 'stage-2'
            }
        }
    }
}