pipeline{
    agent any
    parameters{
        string(name: 'fname', defaultValue: 'Tarvez', description: '')
        string(name: 'lname', defaultValue: 'Khan', description: '')
    }
    stages{
        stage('one'){
            steps{
                sh'''
                "echo Hello ${params.fname}"
                '''
            }
        }
        stage('two'){
            steps{
                echo "Hello ${params.lname}"
            }
        }
    }
}