pipeline{
    agent any
    environment{
        fname="Tarvez"
    }
    stages{
        stage('one'){
                steps{
                    script{
                        if (env.fname == "Tarvez"){
                            echo "Hello ${fname}"
                        }
                            else{
                            echo "You are in Else block"
                    }

                }

            }
            }
        }
}
